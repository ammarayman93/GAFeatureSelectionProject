<div class="container my-3">
    <div class="row">

        <div class="col-md-12">
            <h3 class="py-2">قائمة المستخدمين</h3>
            <hr>
            <table class="table table-bordered" style="vertical-align: middle;">
                <tr>
                    <td>المستخدم</td>
                    <td class="ps-3">معلومات المستخدم</td>
                    
                   
                </tr>
                {{#each users}}


                <tr>
                    
                    <td class="text-center"><i class="far fa-user" style="font-size: 100px;"></i></td>
                    <td class="ps-3">{{this.name}}<br>{{this.email}}<br>{{this.phone}}<br>{{this.phone2}}<br>
                    {{#if this.access}}<span class="text-success">المستخدم يمكنه الوصول إلى الطلبات</span>{{else}}<span class="text-danger">المستخدم لايمكنه الوصول للطلبات</span>{{/if}}
                    </td>
                    
                 
        
                    <td class="text-center"></td>
                    
                </tr>



                {{/each}}
            </table>

        </div>



    </div>
</div>